{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}", "${workspaceFolder}/example"],
      "trace": {
        "logFile": "/mnt/c/Users/conno/Downloads/log.txt"
      },
      // "autoAttachChildProcesses": true,
      "outFiles": ["${workspaceFolder}/out/**/*.js"]
    },
		{
			"name": "Attach",
			"port": 9229,
			"request": "attach",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		}
  ]
}
